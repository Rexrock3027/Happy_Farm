<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/h1301_bac"
    android:orientation="vertical">

    <TextView
        android:id="@+id/h1302_AboutUsTitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginStart="@dimen/dp15"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="@dimen/dp15"
        android:layout_marginBottom="@dimen/dp15"
        android:autoSizeTextType="uniform"
        android:background="#FFFFFF"
        android:backgroundTint="@color/Brown"
        android:text="@string/AboutUsTitle"
        android:textColor="@color/white"
        android:textSize="24sp" />

    <ScrollView
        android:id="@+id/h1302_sc01"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_below="@+id/h1302_AboutUsTitle"
        android:layout_marginStart="@dimen/dp15"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="@dimen/dp15"
        android:background="#FFFFFF"
        android:backgroundTint="@color/Brown_ligmt">

        <TextView
            android:id="@+id/j1302_AboutUsContent"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp10"
            android:layout_marginEnd="@dimen/dp10"
            android:autoSizeTextType="uniform"
            android:text="@string/AboutUsContent"
            android:textSize="14sp" />
    </ScrollView>

    <TextView
        android:id="@+id/h1302_AboutUsInf"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/h1302_sc01"
        android:layout_marginStart="@dimen/dp15"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="@dimen/dp15"
        android:layout_marginBottom="@dimen/dp15"
        android:autoSizeTextType="uniform"
        android:background="#FFFFFF"
        android:backgroundTint="@color/Brown"
        android:text="@string/AboutUsInf"
        android:textColor="#FFFFFF"
        android:textColorHighlight="@color/white"
        android:textSize="24sp" />

    <ScrollView
        android:id="@+id/h1302_sc02"
        android:layout_width="match_parent"
        android:layout_height="126dp"
        android:layout_below="@+id/h1302_AboutUsInf"
        android:layout_marginStart="@dimen/dp15"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="@dimen/dp15"
        android:background="#FFFFFF"
        android:backgroundTint="@color/Brown_ligmt">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TableRow
