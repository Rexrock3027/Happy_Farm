            </TableRow>

        </LinearLayout>
    </ScrollView>

    <LinearLayout
        android:id="@+id/h1301-lh03"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="-15dp"
        android:layout_marginEnd="-15dp"
        android:layout_marginBottom="0dp"
        android:background="@color/Brown"
        android:orientation="horizontal"
        android:paddingStart="5dp"
        android:paddingEnd="5dp"
        android:paddingBottom="5dp">

        <com.city2farmer.CircleImgView
            android:id="@+id/h1302_icon"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_gravity="center"
            android:layout_marginStart="45dp"
            android:layout_marginTop="2dp"
            android:layout_marginEnd="40dp"
            android:scaleType="fitCenter"
            android:scaleX="0.9"
            android:scaleY="0.9"
            android:scrollIndicators="none"
            android:scrollbarDefaultDelayBeforeFade="400"
            android:scrollbarFadeDuration="250"
            app:srcCompat="@drawable/googleg_color" />

        <ImageButton
            android:id="@+id/h1301_b007"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_gravity="center"
            android:layout_marginTop="3dp"
            android:layout_weight="1.0"
            android:background="#00000000"
            android:contentDescription="@string/h1301_t001"
            android:scaleType="fitCenter"
            android:scaleX="1.05"
            app:srcCompat="@drawable/h1301_btn007" />

        <ImageButton
            android:id="@+id/h1301_b008"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_gravity="center"
            android:layout_marginTop="3dp"
            android:layout_weight="1.0"
            android:background="#00000000"
            android:contentDescription="@string/h1301_t010"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/h1301_btn008" />

        <ImageButton
            android:id="@+id/h1301_t001"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_gravity="center"
            android:layout_marginTop="3dp"
            android:layout_weight="1.0"
            android:background="#00000000"
            android:contentDescription="@string/h1301_t010"
            android:scaleType="fitCenter"
            android:visibility="gone"
            app:srcCompat="@drawable/about_us" />

        <ImageButton
            android:id="@+id/h1301_b009"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_weight="1.0"
            android:background="#00000000"
            android:contentDescription="@string/h1301_t010"
            android:scaleType="fitCenter"
            android:visibility="gone"
            app:srcCompat="@drawable/googleg_color" />

