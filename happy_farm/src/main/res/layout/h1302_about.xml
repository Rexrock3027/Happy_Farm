                android:layout_height="wrap_content"
                android:layout_margin="@dimen/dp05">

                <TextView
                    android:id="@+id/h1302_AboutUS05Name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/dp05"
                    android:autoSizeTextType="uniform"
                    android:text="@string/AboutUS05Name"
                    android:textSize="13sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/h1302_AboutUS05Phone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/dp05"
                    android:autoLink="phone"
                    android:autoSizeTextType="uniform"
                    android:text="@string/AboutUS05Phone"
                    android:textColor="@color/Blue"
                    android:textSize="13sp" />

                <TextView
                    android:id="@+id/h1302_AboutUS05Mail"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/dp05"
                    android:autoLink="email"
                    android:autoSizeTextType="uniform"
                    android:text="@string/AboutUS05Mail"
                    android:textSize="13sp" />

            </TableRow>

