<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/m1301_iv01"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/h1301_bac"
    android:scrollbarAlwaysDrawHorizontalTrack="false"
    android:scrollbarAlwaysDrawVerticalTrack="false"
    android:visibility="visible"
    tools:context=".H1301">


    <LinearLayout
        android:id="@+id/h1301_lv01"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="30dp"
        android:orientation="vertical"
        android:padding="5dp">

        <TableRow
            android:id="@+id/h1301_tab02"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/h1301_wt01"
                android:layout_width="175dp"
                android:layout_height="100dp"
                android:layout_marginStart="50dp"
                android:text="@string/h1301_wt01"
                android:textColor="@color/black"
                android:textSize="75sp"
                app:autoSizeTextType="uniform" />

            <ImageView
                android:id="@+id/h1301_i001"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_gravity="center"
                android:contentDescription="@string/h1301_i001"
                app:srcCompat="@drawable/h1301_weasunny" />

        </TableRow>

        <TableLayout
            android:id="@+id/h1301_tabL01"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="gone">

        </TableLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/h1301_wt12"
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="50dp"
                android:text="@string/h1301_wt10"
                android:textColor="@color/black"
                android:textSize="18sp"
                android:visibility="gone"
                app:autoSizeTextType="uniform" />

            <TextView
                android:id="@+id/h1301_wt13"
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:text="@string/h1301_wt11"
                android:textColor="@color/black"
                android:textSize="18sp"
                android:visibility="gone"
                app:autoSizeTextType="uniform" />

            <TextView
                android:id="@+id/h1301_wt05"
                android:layout_width="175dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="50dp"
                android:text="@string/h1301_t012"
                android:textColor="@color/black"
                android:textSize="18sp"
                android:textStyle="bold"
                app:autoSizeTextType="uniform" />

            <TextView
                android:id="@+id/h1301_wt04"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:gravity="center|start"
                android:text="@string/h1301_wt05"
                android:textColor="@color/black"
                android:textSize="18sp"
                android:textStyle="bold"
                app:autoSizeTextType="uniform" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="gone">

            <TextView
                android:id="@+id/h1301_wt02"
                android:layout_width="80dp"
                android:layout_height="40dp"
                android:layout_marginStart="50dp"
                android:text="@string/h1301_wt02"
                android:textColor="@color/black"
                android:textSize="24sp"
                android:visibility="gone"
                app:autoSizeTextType="uniform" />

            <TextView
                android:id="@+id/h1301_wt03"
                android:layout_width="80dp"
                android:layout_height="40dp"
                android:layout_marginStart="0dp"
                android:text="@string/h1301_wt03"
                android:textColor="@color/black"
                android:textSize="24sp"
                android:visibility="gone"
                app:autoSizeTextType="uniform" />

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/h1301_lh01"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/h1301_lv01"
        android:layout_marginTop="60dp"
        android:orientation="horizontal">

        <ImageButton
            android:id="@+id/h1301_b001"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_weight="1.0"
            android:background="#00000000"
            android:contentDescription="@string/h1301_t010"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/h1301_btn001" />

        <ImageButton
            android:id="@+id/h1301_b002"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_weight="1.0"
            android:background="#00000000"
            android:contentDescription="@string/h1301_t010"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/h1301_btn002_2" />

        <ImageButton
            android:id="@+id/h1301_b003"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_weight="1.0"
            android:background="#00000000"
            android:contentDescription="@string/h1301_t010"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/h1301_btn003" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/h1301_lh02"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/h1301_lh01"
        android:layout_marginTop="15dp"
        android:orientation="horizontal">

        <ImageButton
            android:id="@+id/h1301_b004"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_weight="1"
            android:background="#00000000"
            android:contentDescription="@string/h1301_t010"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/h1301_btn004" />

        <ImageButton
            android:id="@+id/h1301_b005"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_weight="1"
            android:background="#00000000"
            android:contentDescription="@string/h1301_t010"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/h1301_btn005" />

        <ImageButton
            android:id="@+id/h1301_b006"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_weight="1"
            android:background="#00000000"
            android:contentDescription="@string/h1301_t010"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/h1301_btn006" />

    </LinearLayout>

    <TextView
        android:id="@+id/h1301_tt010"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/h1301_lh02"
        android:layout_alignParentStart="true"
        android:layout_marginTop="25dp"
        android:gravity="start"
        android:paddingStart="15dp"
        android:paddingEnd="15dp"
        android:text="@string/h1301_t010"
        android:textColor="@color/Brown_ligmt"
        android:textSize="12sp"
        android:visibility="invisible"
        app:autoSizeTextType="uniform" />

    <TextView
        android:id="@+id/h1301_tt011"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/h1301_tt010"
        android:layout_alignParentStart="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="15dp"
        android:gravity="start"
        android:paddingStart="15dp"
        android:paddingEnd="15dp"
        android:text="@string/h1301_t011"
        android:textColor="@color/Brown_ligmt"
        android:textSize="12sp"
        android:visibility="invisible"
        app:autoSizeTextType="uniform" />

    <TextView
        android:id="@+id/h1301_v001"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:background="@color/Brown"
        android:text="@string/h1301_v001" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="-15dp"
        android:layout_marginEnd="-15dp"
        android:layout_marginBottom="5dp"
        android:backgroundTint="@color/Brown"
        android:orientation="horizontal"
        android:paddingStart="5dp"
        android:paddingEnd="5dp"
        android:paddingBottom="5dp">

        <com.city2farmer.CircleImgbutton
            android:id="@+id/h1301_icon"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="50dp"
            android:layout_marginTop="2dp"
            android:layout_marginEnd="40dp"
            android:background="#00000000"
            android:scaleType="fitCenter"
            android:scaleX="0.9"
            android:scaleY="0.9"
            app:srcCompat="@drawable/googleg_color" />

        <ImageButton
            android:id="@+id/h1301_b009"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_weight="1"
            android:background="#00000000"
            android:contentDescription="@string/h1301_t010"
            android:scaleType="fitCenter"
            android:visibility="gone"
            app:srcCompat="@drawable/blank" />

        <ImageButton
            android:id="@+id/h1301_b008"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_marginTop="4dp"
            android:layout_weight="1.0"
            android:background="#00000000"
            android:contentDescription="@string/h1301_t010"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/h1301_btn008" />

        <ImageButton
            android:id="@+id/h1301_t001"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_marginTop="4dp"
            android:layout_weight="1.0"
            android:background="#00000000"
            android:contentDescription="@string/h1301_t010"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/about_us" />

    </LinearLayout>

</RelativeLayout>